	<queryString>
		<![CDATA[SELECT pers.apellidos,pers.nombres, pers.documento_identidad as dni,carg.denominacion,
		as_per.fecha,as_per.entrada,as_per.salida
					FROM asistencia_personal as_per					
					left join personal per on as_per.personal_id = per.id
                    left join persona pers on per.persona_id = pers.id
					left join cargo carg on as_per.cargo_id=carg.id	
					WHERE $P!{whereCond} 	
					order by  pers.apellidos]]>
	</queryString>
